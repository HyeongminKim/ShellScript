					<string>cd "$1" &amp;&gt; /dev/null
if [ "$?" != "0" ]; then
    echo "디렉토리에 접근할 수 없거나 존재하지 않는 디렉토리입니다. $1" &gt;&amp;2
    exit 1
fi

if [ ! -d "$3/Engine" ] ; then
    echo "유효한 UnrealEngine 디렉토리가 아니거나, 잘못된 디렉터리를 선택하였습니다." &gt;&amp;2
    exit 1
fi

if [ ! -x "$3/Engine/Build/BatchFiles/Mac/GenerateProjectFiles.sh" ]; then
    echo "프로젝트 컴파일러에 접근할 수 없거나 UnrealEngine이 아직 빌드되지 않았습니다." &gt;&amp;2
    exit 1
fi

projectName=$(basename "$1")

"$3/Engine/Build/BatchFiles/Mac/GenerateProjectFiles.sh" -project="$1/$projectName.uproject" -game
if [ "$?" != "0" ]; then
    echo "$projectName 프로젝트 빌드 실패. 자세한 정보는 GenerateProjectFiles 문서를 참조해 주세요." &gt;&amp;2
    exit 1
fi

